{
  "name": "techlanding",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init_lms",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:reset": "prisma migrate reset --force",
    "prisma:studio": "prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:security": "npm audit --audit-level=moderate",
    "docker:dev": "docker-compose --profile development up -d",
    "docker:prod": "docker-compose --profile production up -d",
    "docker:build": "docker build -t techgetafrica-lms .",
    "docker:stop": "docker-compose down",
    "analyze": "npm run build && npx @next/bundle-analyzer",
    "security:audit": "npm audit && npm run test:security",
    "validate": "npm run type-check && npm run lint && npm run test",
    "prepare:production": "npm run validate && npm run build"
  },
